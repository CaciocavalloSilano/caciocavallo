<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>Caciocavallo</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>Caciocavallo</name>
            <properties>
                <property name="root">../..</property>
            </properties>
            <folders>
                <source-folder>
                    <label>Caciocavallo</label>
                    <location>${root}</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Caciocavallo</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>SDL classes</label>
                    <location>${root}/src/sdl/classes</location>
                </source-folder>
                <source-folder>
                    <label>Shared classes</label>
                    <type>java</type>
                    <location>${root}/src/share/classes</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>X11 classes</label>
                    <type>java</type>
                    <location>${root}/src/x11/classes</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Test classes</label>
                    <type>java</type>
                    <location>${root}/src/test/classes</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <build-folder>
                    <location>${root}/build/shared/classes</location>
                </build-folder>
                <build-folder>
                    <location>${root}/build/x11/classes</location>
                </build-folder>
                <build-folder>
                    <location>${root}/build/sdl/classes</location>
                </build-folder>
                <build-folder>
                    <location>${root}/build/test/classes</location>
                </build-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>nbproject/nbjdk.xml</script>
                    <target>compile</target>
                </action>
                <action name="clean">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>nbproject/nbjdk.xml</script>
                    <target>javadoc</target>
                </action>
                <action name="run">
                    <script>nbproject/nbjdk.xml</script>
                    <target>run</target>
                </action>
                <action name="test">
                    <script>nbproject/nbjdk.xml</script>
                    <target>run-single-test</target>
                </action>
                <action name="rebuild">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                    <target>compile</target>
                </action>
                <action name="debug">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="run.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>run-selected-file-in-tests</target>
                    <context>
                        <property>run.class</property>
                        <folder>${root}/src/test/classes</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>${root}/build/shared/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>compile</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>${root}/build/x11/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>compile</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>${root}/build/sdl/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>compile</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Shared classes</label>
                        <location>${root}/src/share/classes</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>X11 classes</label>
                        <location>${root}/src/x11/classes</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>SDL classes</label>
                        <location>${root}/src/sdl/classes</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Test classes</label>
                        <location>${root}/src/test/classes</location>
                    </source-folder>
                    <source-file>
                        <label>Central build script</label>
                        <location>${root}/build.xml</location>
                    </source-file>
                    <source-file>
                        <label>Shared build script</label>
                        <location>${root}/src/share/build.xml</location>
                    </source-file>
                    <source-file>
                        <label>X11 build script</label>
                        <location>${root}/src/x11/build.xml</location>
                    </source-file>
                    <source-file>
                        <label>SDL build script</label>
                        <location>${root}/src/sdl/build.xml</location>
                    </source-file>
                    <source-file>
                        <label>Build properties</label>
                        <location>${root}/build.properties</location>
                    </source-file>
                    <source-file>
                        <label>User properties</label>
                        <location>${root}/user.properties</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/2">
            <compilation-unit>
                <package-root>${root}/src/share/classes</package-root>
                <built-to>../../build/shared/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>${root}/src/x11/classes</package-root>
                <classpath mode="compile">${root}/src/share/classes</classpath>
                <built-to>../../build/x11/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>${root}/src/sdl/classes</package-root>
                <classpath mode="compile">${root}/src/share/classes</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>${root}/src/test/classes</package-root>
                <built-to>../../build/test/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-editor-indent">
                <node name="CodeStyle">
                    <property name="usedProfile" value="default"/>
                    <node name="project">
                        <property name="tab-size" value="8"/>
                        <property name="text-limit-width" value="80"/>
                    </node>
                </node>
                <node name="text">
                    <node name="x-java">
                        <node name="CodeStyle">
                            <node name="project"/>
                        </node>
                    </node>
                </node>
            </module>
        </preferences>
    </configuration>
</project>
